{"ast":null,"code":"export default {\n  name: 'App',\n  data() {\n    return {\n      coins: [],\n      filteredCoins: [],\n      titles: ['#', 'Coins', 'Price', 'Prince Change', '24h Volume'],\n      textSearch: ''\n    };\n  },\n  async mounted() {\n    const res = await fetch('https://api.coingecko.com/api/v3/coins/markets?vs_currency=eur&order=market_cap_desc&per_page=100&page=1&sparkline=false');\n    const data = await res.json();\n    this.coins = data;\n    this.filteredCoins = data;\n  },\n  methods: {\n    searchCoin() {\n      this.coins = this.coins.filter(coin => coin.name.toLowerCase().includes(this.textSearch.toLocaleLowerCase()) || coin.symbol.toLowerCase().includes(this.textSearch.toLocaleLowerCase()));\n    }\n  }\n};","map":{"version":3,"mappings":"AA0CA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAI,GAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,MAAM,EAAE,CAAC,GAAG,EAAE,OAAO,EAAE,OAAO,EAAE,eAAe,EAAE,YAAY,CAAC;MAC9DC,UAAU,EAAE;IACd;EACF,CAAC;EACD,MAAMC,OAAO,GAAG;IACd,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,0HAA0H;IAClJ,MAAMP,IAAG,GAAI,MAAMM,GAAG,CAACE,IAAI,EAAC;IAC5B,IAAI,CAACP,KAAI,GAAID,IAAG;IAChB,IAAI,CAACE,aAAY,GAAIF,IAAG;EAC1B,CAAC;EACDS,OAAO,EAAE;IACPC,UAAU,GAAG;MACX,IAAI,CAACT,KAAI,GAAI,IAAI,CAACA,KAAK,CAACU,MAAM,CAACC,IAAG,IAAKA,IAAI,CAACb,IAAI,CAACc,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACV,UAAU,CAACW,iBAAiB,EAAE,KAAKH,IAAI,CAACI,MAAM,CAACH,WAAW,EAAE,CAACC,QAAQ,CAAC,IAAI,CAACV,UAAU,CAACW,iBAAiB,EAAE,CAAC;IACzL;EACF;AACF","names":["name","data","coins","filteredCoins","titles","textSearch","mounted","res","fetch","json","methods","searchCoin","filter","coin","toLowerCase","includes","toLocaleLowerCase","symbol"],"sourceRoot":"","sources":["/home/milan/Escritorio/Dev/VueJS/vue-coingecko-api/src/App.vue"],"sourcesContent":["<template>\n  <div class=\"container my-5\">\n\n    <h1>CoinMarket with VueJSðŸ’š</h1>\n\n    <input type=\"text\" class=\"form-control bg-light rounded border my-3\" placeholder=\"Bitcoin, Ethereum, Dogecoin, Cardano...\" @keyup=\"searchCoin()\" v-model=\"textSearch\">\n\n    <table class=\"table table-striped\">\n      <thead>\n        <tr>\n          <th v-for=\"title in titles\" :key=\"title\">\n            {{ title }}\n          </th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"(coin, index) in coins\" :key=\"coin.id\">\n          <td>\n            {{ index + 1 }}\n          </td>\n          <td>\n            <img :src=\"coin.image\" style=\"width: 2rem;\" class=\"me-2\">\n            <span>{{ coin.name }}</span> <span class=\"text-muted text-uppercase\">{{ coin.symbol }}</span>\n          </td>\n          <td>\n            {{ coin.current_price }} â‚¬\n          </td>\n          <td :class=\"[coin.price_change_percentage_24h > 0 ? 'text-success' : 'text-danger']\">\n            {{ coin.price_change_percentage_24h }} %\n          </td>\n          <td>\n            {{ coin.total_volume }} â‚¬\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n\n</template>\n\n<script>\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      coins: [],\n      filteredCoins: [],\n      titles: ['#', 'Coins', 'Price', 'Prince Change', '24h Volume'],\n      textSearch: ''\n    }\n  },\n  async mounted() {\n    const res = await fetch('https://api.coingecko.com/api/v3/coins/markets?vs_currency=eur&order=market_cap_desc&per_page=100&page=1&sparkline=false')\n    const data = await res.json()\n    this.coins = data\n    this.filteredCoins = data\n  },\n  methods: {\n    searchCoin() {\n      this.coins = this.coins.filter(coin => coin.name.toLowerCase().includes(this.textSearch.toLocaleLowerCase()) || coin.symbol.toLowerCase().includes(this.textSearch.toLocaleLowerCase())) \n    }\n  }\n}\n</script>\n\n<style>\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}